{
  "name": "thedeck",
  "version": "0.1.0",
  "private": true,
  "main": "./electron/main.js",
  "homepage": "./",
  "build": {
    "extends": null,
    "appId": "com.macrodeck",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": -1000
      }
    },
    "coverageReporters": [
      "text",
      "json",
      "html"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.24.0",
    "chroma-js": "^2.1.2",
    "cors": "^2.8.5",
    "electron-json-storage": "^4.5.0",
    "file-saver": "^2.0.5",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.21",
    "node-abi": "^3.5.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "obs-websocket-js": "^4.0.3",
    "ping": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-loading-skeleton": "^3.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-toastify": "^8.1.0",
    "robotjs": "^0.6.0",
    "socket.io": "^4.4.1",
    "styled-components": "^5.3.3",
    "time-stamp": "^2.2.0",
    "tmi.js": "^1.8.5",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "both": "concurrently \"npm start\" \"env APP_DEV=true electron .\"",
    "chill": "run-s start build",
    "build": "react-scripts build",
    "dev": "env APP_DEV=true electron .",
    "dist": "electron-builder",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "npm rebuild --runtime=electron --target=16.0.7 --disturl=https://atom.io/download/atom-shell --abi=99",
    "start": "react-scripts start .",
    "test": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
    "@types/enzyme": "^3.10.11",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.20",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.20",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "cpx": "^1.5.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5"
  }
}
